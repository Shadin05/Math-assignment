#include<iostream>
using namespace std;
int main ()
{
    int r1,c1,r2,c2;
    cout << "rows of matrix 1:";
    cin >> r1;
    cout << "cols of matrix 1";
    cin >> c1;
    cout << "rows of matrix 2:";
    cin >> r2;
    cout << "cols of matrix 2:";
    cin >> c2;

    if (c1 != r2){
        cout << "Multiplication not possible";
    return 0;
    }
    int A[r1][c1], B[r2][c2],M[r1][c2];

    cout << "\nenter matirx 1:\n";
    for( int i = 0; i < r1;i++)
        for(int j = 0; j < c1;j++)
            cin >> A[i][j];

        cout <<"\nenter matrix 2:\n";
        for (int i = 0; i < r2;i++)
            for (int j = 0; j < c2; j++)

                    cin >> B[i][j];

                for(int i = 0; i < r1;i++)
                    for(int j = 0; j < c2; j++)
                    M[i][j] = 0;
                cout << "\nproduct of the two matrices:\n";
                for(int i = 0; i < r1; i++){
                        for(int j = 0; j < c2; j++){
                                for(int k = 0; k < c1;k++){
                                        M[i][j] +=A[i][k]*B[k][j];
                                }
                                cout << M[i][j] << " ";
                        }
                        cout << endl;
                }

            return 0;

}

